<Window x:Class="Terminalapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Terminalapp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="LightSeaGreen">
            <Label  Content="Терминал погрузки и разгрузки" HorizontalAlignment="Center" VerticalAlignment="Center" Width="250" FontSize="16"/>
        </Grid>
        <Frame x:Name="FrameMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrameMain_ContentRendered"/>
    </Grid>
</Window>
